        if(component==='couponUpdateForm'){ %>
        <%-include('./components/manager-coupon-update') %> <%} %><%
        if(component==='couponRegister'){ %>
        <%-include('./components/manager-coupon-register') %> <%} %>
        <%if(component==='users'){ %> <%-include('./components/manager-users')
