  </head>
  <!-- css/management.css -->
  <body>
    <section class="main">
      <%-include('./components/manager-menu') %>

      <div class="mid">
        <% if(component==='main'){ %> <%-include('./components/manager-main') %>
        <%} %> <% if(component==='camps'){ %>
        <%-include('./components/manager-camp') %> <%} %> <%
        if(component==='campRegister'){ %>
        <%-include('./components/manager-camp-register') %> <%} %> <%
        if(component==='camp'){ %>
        <%-include('./components/manager-camp-detail') %> <%} %> <%
        if(component==='campUpdateForm'){ %>
